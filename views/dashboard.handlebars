                                <form action="/api/timeline" method="POST">
                                    <input id="userIdDashboard" type="hidden" name="userId" value={{user}}>
                                    <div class="form-group">
                                        <label>Name</label>
                                        <input class="au-input au-input--full" type="text" name="name" placeholder="Timeline Name...">
                                    </div>
                                    <div class="form-group">
                                        <label>Description</label>
                                        <input class="au-input au-input--full" type="text" name="description"
                                            placeholder="Timeline Description...">
                                    </div>
                                    <div class="form-group">
                                        <input id="timeLimitCheck" type="checkbox" value=0> Enable Time Limit
                                    </div>
                                    <div class="form-group" id="hiddenTimelimit">
                                        <input class="au-input au-input--full" type="number" value=0 name="timelimit"
                                            placeholder="Time Limit Here (Days)">
                                    </div>
                                    <div class="form-group">
                                        <input id="publicCheck" name="public" type="checkbox" value=1> Public
                                    </div>
                                    <div class="form-group" id="editableCheck">
                                        <input type="checkbox" name="editable" id="editableCheck2" value=1> Allow
                                        others to add events
                                    </div>

                                    <button class="au-btn au-btn--block au-btn--green m-b-20" id="newTimelineSubmit">Create</button>

                                </form>
                            </div>
                        </div>
                    </div>
                </div>
                <!--End Modal Content-->
            </div>
        </div>
    </div>
